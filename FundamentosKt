//-----------------------------------------------------------------------
fun main() {
   
    //Exercício -> No metroid vania, para a tela de game over, seus corações precisam ser igual a zero e 
    //suas vidas precisam ser menor ou igual a zero
    
    //simule com operadores logicos quando ele vai para a game over e quando ele sai
  /*  
    var coracoes = 1
    var vidas = 3
    
    if(coracoes == 0 <= 0){
        println("game over !!")
    }else{
        println("o jogo continua")
    }
    */
    
    //When = Quando
    //
    //coparação de quando um valor for igual e um determinado valor,
    //uma determinada ação sera executada 
    //
    //estrutura do When
    //
    //var auxiliar = valor
    //
    //When(auxiliar){
    //
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //valor -> {ação}
    //else -> {ação}
    //}
    //sará usado geralmente quando durante o programa o valor da variavel
    //não for estetico, ou seja, ele estiver mudando
    //
    
    var mes = 5
    
    when(mes){
       1 -> {println("Lisboa")} 
       2 -> {println("Nova York")} 
       3 -> {println("Rio Janeiro")} 
       4 -> {println("São Paulo")}
       5 -> {println("Keyland")} 
       6 -> {println("Miwaukee") }
       7 -> {println("Barcelona")}
       8 -> {println("Springfield")}
       9 -> {println("Los Pollos Hermanos")}
       10 -> {println("Elmore")}
       11 -> {println("Toronto")}
       12 -> {println("Seoul")}
       else -> {println("Acordou")}
    }
    
    
    
    
    
    
    
    
    
    
    
    
}




//---------------------------------------------------------------------------
fun main() {
    //exercicio -> dia 10/04
   
    var diasemana = 1
    
    when(diasemana){
        
        1 -> println("Domingo, não é util igual vc")
        2 ->println("Segunda, dia util, pra quem?")
        3->println("terça, é util mas ninguem lembra")
        4 ->println("Quarta, é util e a semana já ta no meio")
        5 ->println("quinta, é util mas quem se importa")
        6 ->println("sexta, é util, melhor dia")
        7 ->println("sabado, não é util, não sou eu que to dizendo")
        8 ->println("feriado, não é util mas é muito bom")
        
    }
    
    when(diasemana){
        2,
        3,
        4,
        5,
        6 -> {println("dia util seja produtivo")}
        else ->{println("fim de semana")}
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
}
//-----------------------------------------------------------------------------------------------------------------------------
fun main() {
    
    // Laços de repetição - Loops 
    // 
    // repeat - repita
    // while - enquanto
    // do..while - faça..enquanto
    // for - para 
    // 
    //repeat - repita
    //vai repetir uma determinada ação uma determinada quantidade de vezes 
    //
    //Estrutura do repeat 
    //
    // repeat(númeroVezes){
    // ação
    // } 
    /*
    repeat(8){
        println("Isso é um teste, testando, testando")
    } 
    */
    // Exercício -> faça um programa com auxilio de uma variavel contador,
    // no qual nela será acrescentado 1 a cada repetição,
    // até que dê a quantidade de vezes decidida 
    /*
    var contador = 0 
    repeat(10){
        println(contador++)
    }
    */ 
    //Laço while - enquanto 
    //
    // enquanto uma condição for verdadeira, uma determinada ação estará sendo executada
	// Estrutura do while 
	// 
	// while(condição){
	// ação
	// } 
	/*var contador = 0
    while(contador < 10){
        println(contador++)
    } 
    */
    //Exercício -> Simulador de Bateria de celular
    // faça um programa com while que mostre s bateria 100% diminuindo. 
    // Quando ela chegar no 15, deve mostrar a mesnsagem pedindo para 
    // conectar o carrgador, e quando chegar a 0, deve mostrar a mesnsagem
    // aparelho desligando
    /*
    var bateria = 100
    while(bateria >= 0){
        println("$bateria%")
        
        when(bateria){
           15 -> {println("Aparelho descarregando, conecte no carregador!")} 
           0 -> {println("Aparelho desligado")}
        }
        
        bateria--
    }
    */
    //while -> Enquanto uma determinada condição for vdd, uma ação sera executada
    
    //do..while
    //
    //uma determinada ação sera executado, enquanto uma determinada condição for vdd
    //
    //Estrutura:
    //
    //do{
    //ação
    //}while(condição)
   /* 	var contador = 0
    while(contador < 10){
        println(contador++)
    } 
    */
   /*
    var contador = 0
    do{
        println(contador++)
    }while(contador > 10)
    */
   
    //for - para
    //para cada item em um intervalo ou colection, uma determinada ação sera executada
    //
    //estrutura:
    //
    //for(vAuxiliar in intervalo colection){
    //ação
    //}
    //
    
    for(pneu in 1..4){
        println("$pneu")
    }
    
    //exercicio -> faça uma tabuada até o decimo multiplo de um numero escolhido,
    //utilizando o laço
    
    
    
    
    
    
    
}
