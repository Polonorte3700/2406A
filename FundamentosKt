fun main() {
    
    //exercicio -> faça o catalogo ficticio de animais que estão no zoologico de sp
    //com no minimo 20 animais, sem repetição de raça
    
    var animal1 = animal("gato","persa",7,"ratos","pequeno")
    var animal2 = animal("gato","Minie coon",9,"variado","grande")
    var animal3 = animal("cachorro","não definida",10,"qualquer coisa","variado")
    var animal4 = animal("dog","pincher",90,"chinelo","pequeno")
    var animal5 = animal("gato","siamês",9,"uiscas saxe","pequeno")
    var animal6 = animal("gato","ragdoll",6,"variado","pequeno")
    var animal7 = animal("gato","sphynx",4,"variado","pequno")
    var animal8 = animal("dog","golden",3,"variado","grande")
    var animal9 = animal("dog","chitso",2,"variado","pequeno")
    var animal10 = animal("dog","pastor alemão",13,"variado","pequena")
    var animal11 = animal("dog","yasa",9,"variado","pequeno")
